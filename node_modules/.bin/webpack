#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/bin/webpack.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules:/home/alex/projects/mapbox/node_modules/webpack/bin/webpack.js/node_modules:/home/alex/projects/mapbox/node_modules/webpack/bin/node_modules:/home/alex/projects/mapbox/node_modules/webpack/node_modules" "$basedir/node"  "$basedir/../webpack/bin/webpack.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/bin/webpack.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules/webpack/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/1.15.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules:/home/alex/projects/mapbox/node_modules/webpack/bin/webpack.js/node_modules:/home/alex/projects/mapbox/node_modules/webpack/bin/node_modules:/home/alex/projects/mapbox/node_modules/webpack/node_modules" node  "$basedir/../webpack/bin/webpack.js" "$@"
  ret=$?
fi
exit $ret
