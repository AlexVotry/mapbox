#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/6.26.0/node_modules/babel-cli/bin/babel.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/6.26.0/node_modules/babel-cli/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/6.26.0/node_modules/babel-cli/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/6.26.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules:/home/alex/projects/mapbox/node_modules/babel-cli/bin/babel.js/node_modules:/home/alex/projects/mapbox/node_modules/babel-cli/bin/node_modules:/home/alex/projects/mapbox/node_modules/babel-cli/node_modules" "$basedir/node"  "$basedir/../babel-cli/bin/babel.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/6.26.0/node_modules/babel-cli/bin/babel.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/6.26.0/node_modules/babel-cli/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/6.26.0/node_modules/babel-cli/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/6.26.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/babel-cli/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules:/home/alex/projects/mapbox/node_modules/babel-cli/bin/babel.js/node_modules:/home/alex/projects/mapbox/node_modules/babel-cli/bin/node_modules:/home/alex/projects/mapbox/node_modules/babel-cli/node_modules" node  "$basedir/../babel-cli/bin/babel.js" "$@"
  ret=$?
fi
exit $ret
