#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/1.16.5/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/1.16.5/node_modules/webpack-dev-server/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/1.16.5/node_modules/webpack-dev-server/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/1.16.5/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules:/home/alex/projects/mapbox/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/alex/projects/mapbox/node_modules/webpack-dev-server/bin/node_modules:/home/alex/projects/mapbox/node_modules/webpack-dev-server/node_modules" "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/1.16.5/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/1.16.5/node_modules/webpack-dev-server/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/1.16.5/node_modules/webpack-dev-server/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/1.16.5/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/webpack-dev-server/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules:/home/alex/projects/mapbox/node_modules/webpack-dev-server/bin/webpack-dev-server.js/node_modules:/home/alex/projects/mapbox/node_modules/webpack-dev-server/bin/node_modules:/home/alex/projects/mapbox/node_modules/webpack-dev-server/node_modules" node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
  ret=$?
fi
exit $ret
