#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/rimraf/bin.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/rimraf/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/1.2.9/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../rimraf/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/rimraf/bin.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/rimraf/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/1.2.9/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/fsevents/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" node  "$basedir/../rimraf/bin.js" "$@"
  ret=$?
fi
exit $ret
