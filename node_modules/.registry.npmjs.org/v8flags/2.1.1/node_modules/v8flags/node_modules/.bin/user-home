#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/user-home/1.1.1/node_modules/user-home/cli.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/user-home/1.1.1/node_modules/user-home/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/user-home/1.1.1/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/user-home/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../user-home/1.1.1/node_modules/user-home/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/user-home/1.1.1/node_modules/user-home/cli.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/user-home/1.1.1/node_modules/user-home/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/user-home/1.1.1/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/user-home/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../user-home/1.1.1/node_modules/user-home/cli.js" "$@"
  ret=$?
fi
exit $ret
