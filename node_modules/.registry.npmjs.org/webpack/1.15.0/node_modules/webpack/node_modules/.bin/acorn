#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/3.3.0/node_modules/acorn/bin/acorn/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/3.3.0/node_modules/acorn/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/3.3.0/node_modules/acorn/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/3.3.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../acorn/3.3.0/node_modules/acorn/bin/acorn" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/3.3.0/node_modules/acorn/bin/acorn/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/3.3.0/node_modules/acorn/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/3.3.0/node_modules/acorn/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/3.3.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/acorn/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../acorn/3.3.0/node_modules/acorn/bin/acorn" "$@"
  ret=$?
fi
exit $ret
