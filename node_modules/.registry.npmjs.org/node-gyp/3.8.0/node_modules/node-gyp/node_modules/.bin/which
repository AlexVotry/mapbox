#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/1.3.1/node_modules/which/bin/which/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/1.3.1/node_modules/which/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/1.3.1/node_modules/which/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/1.3.1/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../which/1.3.1/node_modules/which/bin/which" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/1.3.1/node_modules/which/bin/which/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/1.3.1/node_modules/which/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/1.3.1/node_modules/which/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/1.3.1/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/which/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../which/1.3.1/node_modules/which/bin/which" "$@"
  ret=$?
fi
exit $ret
