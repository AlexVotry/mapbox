#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules/nopt/bin/nopt.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules/nopt/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules/nopt/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../nopt/3.0.6/node_modules/nopt/bin/nopt.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules/nopt/bin/nopt.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules/nopt/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules/nopt/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/3.0.6/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/nopt/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../nopt/3.0.6/node_modules/nopt/bin/nopt.js" "$@"
  ret=$?
fi
exit $ret
