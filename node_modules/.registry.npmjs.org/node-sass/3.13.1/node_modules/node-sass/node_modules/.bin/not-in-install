#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/in-publish/2.0.0/node_modules/in-publish/not-in-install.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/in-publish/2.0.0/node_modules/in-publish/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/in-publish/2.0.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/in-publish/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../in-publish/2.0.0/node_modules/in-publish/not-in-install.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/in-publish/2.0.0/node_modules/in-publish/not-in-install.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/in-publish/2.0.0/node_modules/in-publish/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/in-publish/2.0.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/in-publish/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../in-publish/2.0.0/node_modules/in-publish/not-in-install.js" "$@"
  ret=$?
fi
exit $ret
