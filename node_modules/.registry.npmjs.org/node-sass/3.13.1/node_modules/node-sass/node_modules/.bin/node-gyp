#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/3.8.0/node_modules/node-gyp/bin/node-gyp.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/3.8.0/node_modules/node-gyp/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/3.8.0/node_modules/node-gyp/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/3.8.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../node-gyp/3.8.0/node_modules/node-gyp/bin/node-gyp.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/3.8.0/node_modules/node-gyp/bin/node-gyp.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/3.8.0/node_modules/node-gyp/bin/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/3.8.0/node_modules/node-gyp/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/3.8.0/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node-gyp/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../node-gyp/3.8.0/node_modules/node-gyp/bin/node-gyp.js" "$@"
  ret=$?
fi
exit $ret
