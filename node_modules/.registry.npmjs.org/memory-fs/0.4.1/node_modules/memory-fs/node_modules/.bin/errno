#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/errno/0.1.7/node_modules/errno/cli.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/errno/0.1.7/node_modules/errno/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/errno/0.1.7/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/errno/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../errno/0.1.7/node_modules/errno/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/errno/0.1.7/node_modules/errno/cli.js/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/errno/0.1.7/node_modules/errno/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/errno/0.1.7/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/errno/node_modules:/home/alex/projects/mapbox/node_modules/.registry.npmjs.org/node_modules:/home/alex/projects/mapbox/node_modules:/home/alex/projects/node_modules:/home/alex/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../errno/0.1.7/node_modules/errno/cli.js" "$@"
  ret=$?
fi
exit $ret
